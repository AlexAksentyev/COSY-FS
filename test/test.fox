INCLUDE 'bin/setups/BNL';

PROCEDURE RUN;
  VARIABLE WHERE 100;
  VARIABLE TILTS 32; VARIABLE I 1; VARIABLE J 1;
  VARIABLE F 100 6; VARIABLE A 100 2; VARIABLE B 100 2;
  VARIABLE G 100 2; VARIABLE R 100 2; VARIABLE MU 100 2; 

  PROCEDURE INJECT_DELTA NUM;
    VARIABLE X 100; VARIABLE I 1;
      X := LINSPACE(-1E-4, 1E-4, NUM);
    LOOP I 1 NUM;
      SR 0 0 0 0 0 X|I 0 0 1; SSR 0 0 1;
    ENDLOOP;
  ENDPROCEDURE;

  DIRSET WHERE 'data/TEST/' TRUE;
  GROUTF 'img/dump/TR' 1;

  OV 3 2 0;
  RP 270 1876.5592/AMUMEV 1;
  RPS 1 -.142987;


  CW_SETUP 0 0 0 ZEROS(32); GT MAP F MU A B G R;
  WRITE 6 CONS(B(1)) CONS(B(2));

  INJECT_DELTA 10;
  TRACK 1000 1 WHERE '' 0;
  
  WRITE 6 'SUCCESS!';
  
ENDPROCEDURE; {RUN}

RUN; END;
